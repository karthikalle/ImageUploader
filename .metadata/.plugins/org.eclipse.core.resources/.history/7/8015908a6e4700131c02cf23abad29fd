
public class FileAndPathOnly {
	private String filePath;
	private String file;
	
	FileAndPathOnly() {
		filePath=null;
	}
	
	public String getFilePath() {
		return filePath;
	}
	
	public void setFilePath(String path) {
		filePath = path;
	}

	public String getFile() {
		return file;
	}

	public void setFile(String file) {
		this.file = file;
	}
}
