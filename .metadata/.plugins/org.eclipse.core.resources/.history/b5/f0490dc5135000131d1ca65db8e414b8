package dataEntryTesting;

import org.junit.Test;

import dataEntry.GUI_DataEntry;
import junit.framework.TestCase;

public class UploadToSimpleDBTest extends TestCase {
	
	@Test
	public void noImageName() {
		GUI_DataEntry gde = new GUI_DataEntry();
		
		gde.imageName.setText("");
		gde.uploadToSimpleDB();
	}
	
	
	public UploadToSimpleDBTest(String name) {
		super(name);
	}
}